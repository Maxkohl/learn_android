<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <SwitchPreferenceCompat
        app:key="sync"
        app:title="@string/sync_title" />


    <ListPreference
        app:defaultValue="US"
        app:entries="@array/market_entries"
        app:entryValues="@array/market_values"
        app:key="market_choice"
        app:title="@string/markets_title"
        app:useSimpleSummaryProvider="true" />

</PreferenceScreen>